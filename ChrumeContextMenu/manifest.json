{
  "manifest_version": 2,

  "name": "Copy URL",
  "description": "This extension is a waste of time",
  "version": "0.095",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
	"contextMenus",
	"tabs"
  ],
  "background": {
    "scripts": ["contextMenu.js"]
  }
}
